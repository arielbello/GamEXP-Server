<% if !@feedback.nil? %>
  <%= form_for(@feedback) do |f| %>
    <% if @feedback.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

        <ul>
        <% @feedback.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

    <div class="field">
      <%= f.label :rating %><br>
      <%= f.number_field :rating %>
    </div>
    <div class="field">
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </div>
    <div class="field">
      <%= f.label :conf_id %><br>
      <%= f.text_field :conf_id %>
    </div>
    <div class="field">
      <%= f.label :player_id %><br>
      <%= f.text_field :player_id %>
    </div>
    <div class="field">
      <%= f.label :game_id %><br>
      <%= f.text_field :game_id %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>

<% elsif !@conf.nil? %>
  <% if @conf.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@conf.errors.count, "error") %> prohibited this feedback from being saved:</h2>
    <ul>
    <% @conf.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
<% end %>
